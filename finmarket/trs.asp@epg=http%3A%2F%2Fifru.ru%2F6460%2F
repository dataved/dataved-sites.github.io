//<![CDATA[

function searchSubmit() {
  
  var inp_f = document.getElementById('search_inp_f');
  
  if (!inp_f && document.getElementById('search_inp').value.match(/^\s*$/)) {
    
    alert('Введите поисковый запрос!');
    
    document.getElementById('search_inp').focus();
    
    return false;
  }
  
  if (inp_f && inp_f.value.match(/^\s*$/)) {
    
    alert('Введите поисковый запрос!');
    
    inp_f.focus();
    
    return false
  }
  
  return  true;
  
}

//]]>
</script>
