		frm["aitype_" + i].selectedIndex = 0;
		frm["aima_" + i].selectedIndex = 0;
		frm["aipar1_" + i].value = "";
		frm["aipar2_" + i].value = "";
	}
	frm.submit();
}
for (i = 0; i < 3; i++) {
	SelectAIType(i);
