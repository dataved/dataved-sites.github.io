			<script type="text/javascript">
			function CalcSell() {
				var frm = document.forms['NVCalc'];
				if (frm) {
					var val1 = frm.val_1_f[frm.val_1_f.selectedIndex].value;
					var val2 = frm.val_1_t[frm.val_1_t.selectedIndex].value;
					var sum = frm.sum_1.value;
					
					DoRequest(val1, val2, sum, 1);
				}
				return false;
			}
			function CalcBuy() {
				var frm = document.forms['NVCalc'];
				if (frm) {
					var val1 = frm.val_2_f[frm.val_2_f.selectedIndex].value;
					var val2 = frm.val_2_t[frm.val_2_t.selectedIndex].value;
					var sum = frm.sum_2.value;
					DoRequest(val1, val2, sum, 0);
				}
				return false;
			}
			function DoRequest(val1, val2, sum, operation_type) {
				if (val1 == val2) {
					alert("Невозможно выполнить расчет:\nвыберите разные валюты, пожалуйста.");
					return false;
				}
				sum = sum.replace(",", ".");
				if (isNaN(sum) || (sum < 1) || (sum > 100000000)) {
					alert("Расчет для указанной суммы невозможен:\nзначение суммы конвертации должно быть в пределах от 1 до 100 000 000");
					return false;
				}
				$("#calcResults").load("/currency/exchange/calc.asp?op=26940&val1=" + val1 + "&val2=" + val2 + "&sum=" + sum + "&ot=" + operation_type, function(resp, status, xmlReq) {
					if (status == "error") {
						$("#calcResults").html("Невозможно выполнить расчет: внутренняя ошибка.");
					}
				});
			}
			CalcSell();
			</script>
			</div>
