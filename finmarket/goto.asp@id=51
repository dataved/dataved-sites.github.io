		$(function(){
			var gridster = $('.gridster ul').gridster({
				widget_margins: [5, 5],
				widget_base_dimensions: [145, 100]
			}).data('gridster');
		 
		 	if (gridster)
			 	gridster.disable();

			$('.section-description-expand').click(function(e){
				
				var $wrap = $('.section-description-wrap');
				var attr = $wrap.attr('style');

				if (!attr)
				{
					$wrap.height($wrap.find('.section-description-text').height());
					$('.sp-expand-down').removeClass('sp-expand-down').addClass('sp-expand-up');
				}
				else
				{
					$wrap.removeAttr('style');
					$('.sp-expand-up').removeClass('sp-expand-up').addClass('sp-expand-down');
				}

				e.preventDefault();
			})
		});
		</script>
