		<img src="http://gr04.finmarket.ru/Charts/TechAnalysis.aspx?src=72&ft=4442&per=4&depth=0&vol=0&ct=1" border="0" id="chart" onload="LoadChartData()" onmousemove="drawValues(event)" onmouseout="HideLine()"><br>
 		<div id="chartLine" style="display:none; width:1px; font-size: 1px; margin: 0px; padding: 0px; background-color:#ff0099; position:absolute; top: 0px; left: 0px;  z-index: 1;">.</div>
		<div id="chartVal" style="display:none; border: 1px solid #000000; font-size: 10px; padding: 1px; background-color:#FFFFCC; color: #000000; font-family: Verdana; position:absolute; top: 0px; left: 0px;  z-index: 250; text-align: left;"></div>
		<script language="javascript" id="jsData"></script>
		<script language="javascript">
		var chartOk = false;
		var prevArrId = -1;
		
		var oLine = document.getElementById("chartLine");
		var oCanvas = document.getElementById("chart"); //document.getElementById("chartCanvas");
		var oChartVal = document.getElementById("chartVal");
		var chart = document.getElementById("chart");
		var jsData = document.getElementById("jsData");

		function LoadChartData() {
			if (jsData) {
				jsData.src="http://gr04.finmarket.ru/Charts/TechAnalysis.aspx?getContent";
			}
		}
		
		function StartDynChart() {
			chartOk = true;
		}
		function drawValues(evt) {
			if (chartOk) {
				var lMouseCoordX = 0;
				var lMouseCoordY = 0;
				if ('layerX' in evt) {
					lMouseCoordX = evt.layerX;
					lMouseCoordY = evt.layerY;
				} else {
					if ('x' in evt) {
						lMouseCoordX = evt.x;
						lMouseCoordY = evt.y;
					}
				}
				if (oLine && oCanvas) {
					if (oLine.style.display == "none") {
						oLine.style.display = "block";
						oLine.style.height = oCanvas.offsetHeight - 121;
						oLine.style.top = (29 + oCanvas.offsetTop);
					}
					var newArrId = getNearestXCoord(lMouseCoordX);
					if (prevArrId != newArrId) {
						prevArrId = newArrId;
						oLine.style.left = arrCoordX[prevArrId] + oCanvas.offsetLeft;
						if (oChartVal) {
							if (oChartVal.style.display == "none") {
								oChartVal.style.display = "block";
								oChartVal.style.top = oCanvas.offsetTop + oCanvas.offsetHeight / 2;
							}
							oChartVal.style.left = 16 + arrCoordX[prevArrId] + oCanvas.offsetLeft + 4;
							oChartVal.style.top = 16 + oCanvas.offsetTop + lMouseCoordY;
							oChartVal.innerHTML = arrValues[prevArrId];
						}
					}
				}
			}
		}
		function HideLine() {
			if (oLine && chartOk) {
				oLine.style.display = "none";
				oChartVal.style.display = "none";
				prevArrId = -1;
			}
		}
		function getNearestXCoord(lMouseX) {
			var diff = 0;
			var arrId = 0;
			var prev = 0;
			for(i = 0; i < arrCoordX.length; i++) {
				diff = Math.abs(lMouseX - arrCoordX[i]);
				if (diff == 0) {
					arrId = i;
					break;
				} else {
					if (i == 0) {
						prev = diff;
					}
					if (diff < prev) {
						arrId = i;
						prev = diff;
					}
				}
			}
			return arrId;
		}
		</script>
		
