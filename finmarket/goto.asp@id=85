        <script type="text/javascript">
            Sys.Application.add_init(application_init);
            loadGallery();
            function application_init() {
                var prm = Sys.WebForms.PageRequestManager.getInstance();
                prm.add_endRequest(prm_endRequest);
            }
            function prm_endRequest() {
                try {
                    loadAll();
                }
                catch (e) { }
                try {
                    setCodeInput();
                }
                catch (e) { }
                loadGallery();
            }
            function loadGallery() {
                $(document).ready(function () {

                    var imgList = $('.banner-scroll a');
                    if (imgList.length) {
                        $('.main-link').attr('href', $(imgList.get(0)).attr('lnk'));
                        $('.main-img').attr('src', $(imgList.get(0)).attr('img'));
                    }

                    $('.banner-scroll a').click(function () {
                        $('.banner-scroll a img').attr('src', '/img/bullet-noactive.png');
                        $(this).find('img').attr('src', '/img/bullet-active.png');

                        $('.main-link').attr('href', $(this).attr('lnk'));
                        $('.main-img').attr('src', $(this).attr('img'));

                    });

                    timer = setTimeout('run();', interval);
                });
            }
            var timer;
            var interval = 7000;
            function run() {
                var next = $('.banner-scroll a img[src="/img/bullet-active.png"]').parent().next();
                if (next.length) next.click();
                else $('.banner-scroll a:first').click();
                
                timer = setTimeout('run();', interval);
            }
        </script>
        
<script type="text/javascript">
    $(document).ready(function() {
        sendLoadData();
    });

    function sendLoadData() {
        var resol = '';
        if (self.screen) {
            width = screen.width;
            height = screen.height;
        }
        if (width && height) resol = width + ' x ' + height;
        $.post("/aspx_script/updatestat.aspx", { Type: 1, RQID: '5d23f07a-ff5d-4893-be3c-88607c9fc231', Screen: resol, Color: screen.colorDepth });

    }
    window.onbeforeunload = function() {
        $.post("/aspx_script/updatestat.aspx", { Type: 2, RQID: '5d23f07a-ff5d-4893-be3c-88607c9fc231' });
    };
</script>


    

