<script type="text/javascript">
	$(document).ready(function() {
		$('.share-init').click(function(e){

			$.get(
				'/getshares/',
				{
					'url': 'http://presents2014.finmarket.ru/rubrics/7'
				},
				function(data){
					$('.share-count-fb').text(data.fb);
					$('.share-count-vk').text(data.vk);
					$('.share-count-tw').text(data.tw);
				}
			);

			$(this).addClass('hidden');
			$('.share-buttons').removeClass('hidden');
			e.preventDefault();
		});

		$('.share-button').click(function(e) {
			var width  = 575,
				height = 400,
				left   = ($(window).width()  - width)  / 2,
				top    = ($(window).height() - height) / 2,
				url    = this.href,
				opts   = 'status=1' +
						 ',width='  + width  +
						 ',height=' + height +
						 ',top='    + top    +
						 ',left='   + left;

			window.open(url, 'share', opts);

			return false;
		});
	});
</script>
