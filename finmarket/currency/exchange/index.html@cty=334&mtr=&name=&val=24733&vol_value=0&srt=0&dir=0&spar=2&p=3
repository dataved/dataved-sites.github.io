			<script type="text/javascript">
			var selectedSubwayStations = ",";
			function ShowMetroMap() {
				if (document.getElementById("metro_content").innerHTML == "") {
					$("#metro_content").load("/currency/exchange/metro_moscow.html", function(resp) {
							document.getElementById("metro_content").innerHTML = resp;
							document.getElementById("metro_content").style.display = "block";
					});
				} else {
					document.getElementById("metro_content").style.display = "block";
				}
				$(".content").hide();
				$("#footer").hide();
				$(".top").hide();
				return false;
			}
			function HideMetroMap() {
				document.getElementById("metro_content").style.display = "none";
				document.forms['nv'].mtr.value = selectedSubwayStations;
				document.forms['nv'].submit();
				return false;
			}
			</script>
	
