			<script type="text/javascript">
			function DoSort(srt) {
				var frm = document.forms['nv'];
				if (frm) {
					if (srt == frm.srt.value) {
						frm.dir.value = 1 - frm.dir.value;
					} else {
						frm.srt.value = srt;
						frm.dir.value = 0;
					}
					frm.submit();
				}
				return false;
			}
			function SelectFields() {
				return true;
			}
			function ResetMetroList() {
				var frm = document.forms['nv'];
				if (frm) {
					frm.mtr.value = "";
					frm.submit();
				}
				return false;
			}
			</script>
