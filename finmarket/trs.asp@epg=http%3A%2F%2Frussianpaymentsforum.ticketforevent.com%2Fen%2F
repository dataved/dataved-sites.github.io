<script type="text/javascript">
  var epts_aid = '';
  var get_aid_matches = /[?&]aid=([\d]*)/.exec(window.location.href);
  if (get_aid_matches && get_aid_matches.length && get_aid_matches[1].length)
    epts_aid = get_aid_matches[1];
  if (epts_aid && epts_aid.length)
    epts_aid += '/';
  window.isWidget = true;
  var epbs_window_height = '800px';
  if(screen.height < 800){
	epbs_window_height = '700px';
  }

if(init_widget_eptsWidget_5989_en == undefined)
 {
  var init_widget_eptsWidget_5989_en = function ()
  {
    if ( (typeof ticketing_widget_js_loaded == 'undefined') || !ticketing_widget_js_loaded)
    {
        setTimeout(init_widget_eptsWidget_5989_en, 100);
        return;
    }

    (function(jQuery){
      var $ = jQuery;

      $('.epts-showPClink').live('click', function(e) {
        $this = $(this);
        $this.addClass('epts-inv');
        $('.epts-pcInput', $this.closest('.epts-buyTicketsWidget')).removeClass('epts-inv').find('input[type=text]').focus();
        e.preventDefault();
      });

      $(document).click(function(e) {
        if ($(e.target).closest('.epts-pcInput, .epts-inv').length == 0)
        {
         $('.epts-pcInput').addClass('epts-inv');
         $('.epts-showPClink').removeClass('epts-inv');
        }
      });

      var tableInit_eptsWidget_5989_en = function()
      {
        $('#eptsWidget_5989_en .epts-typeRow select').change(function () { $(this).blur(); tableUpdate_eptsWidget_5989_en($(this).closest('.epts-buyTicketsWidget')); } );
        $('#eptsWidget_5989_en .epts-goToFormButton .cool-button').bind('click', function (e) {
           var $epts_form = $('.epts-selectionForm', $(this).closest('.epts-buyTicketsWidget'));
           try { _gaq = _gaq || [];
             _gaq.push(['_trackEvent', 'Ticket', 'Click_5989_' + epts_aid, $('.epts-totalRow td:eq(1)', $epts_form).text()]);
             _gaq.push(['b._trackEvent', 'Ticket', 'Click_' + epts_aid, $('.epts-totalRow td:eq(1)', $epts_form).text()]);
           } catch(err) {}
           $(this).blur();
           var loc = $epts_form.attr('target').replace('[AID]', epts_aid)
           loc =   loc  + (loc.indexOf('?') != -1 ? '&' : '?') + $epts_form.find(':input').serialize();
           if (window.isWidget)
             window.open(loc, '_blank', 'width=1000px, height=' + epbs_window_height + ', scrollbars=yes, toolbar=yes, status=yes');
            else
           {
             location.href = loc;
             $(this).addClass('epts-disabled').prop("disabled", true);
           }

          e.preventDefault();
        });
        if (typeof tw_customHue != 'undefined' && typeof tw_customSaturation != 'undefined' && typeof setWidgetColors == 'function')
          setWidgetColors(tw_customHue, tw_customSaturation);
        if (typeof updateWidgetForm == 'function')
          updateWidgetForm();
      }

      var tableUpdate_eptsWidget_5989_en = function(context) {
        var $epts_form = $('.epts-selectionForm', context);
        $('.epts-tableContainer', context).loadP($epts_form.attr('action'), $epts_form.find(':input').serialize(), tableInit_eptsWidget_5989_en);
      };

      tableInit_eptsWidget_5989_en();
      $('#eptsWidget_5989_en .epts-pcInput :button').live('click', function() { tableUpdate_eptsWidget_5989_en($(this).closest('.epts-buyTicketsWidget')) });
      $('#eptsWidget_5989_en .epts-pcInput input[type=text]').live('keypress', function(e) { if(e.which == 13) { tableUpdate_eptsWidget_5989_en($(this).closest('.epts-buyTicketsWidget')); return false; } });

    })($_ticketingWidget);
  };

 }
 init_widget_eptsWidget_5989_en();

</script>

