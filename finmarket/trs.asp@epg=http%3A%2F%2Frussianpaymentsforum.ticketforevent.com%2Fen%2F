                  <link href="http://russianpaymentsforum.ticketforevent.com/css/jquery_ui.css?v=1359631877" rel="stylesheet" type="text/css" />
         <link href="http://russianpaymentsforum.ticketforevent.com/css/tipstyle/tip.css?v=1342442490" rel="stylesheet" type="text/css" />
         <script src="http://russianpaymentsforum.ticketforevent.com/js/jquery.poshytip.min.js?v=1342442485" type="text/javascript"></script>
         <script src="http://russianpaymentsforum.ticketforevent.com/js/jquery_ui.js?v=1342442485" type="text/javascript"></script>
         <script src="http://russianpaymentsforum.ticketforevent.com/js/poshytipInit.js?v=1365698112" type="text/javascript"></script>

            <script type="text/javascript">
                var locationMap = null, locationInfo = null;

                function openMapDialog()
                    {
                        window.$dialog = $('#dialog-content').dialog({
                            title: 'Swissotel Krasnye Holmy, Russia, Moscow, naberezhnaya Kosmodamianskaya 52 строение 6',
                            width: '900px',
//                            buttons: buttons,
                            position: ['center'],
                            resizable: false,
                            minHeight: '500px',
                            modal: true
                        });

                    }

                function locationMapInitialize()
                    {
                        var venuePosition = new google.maps.LatLng(55.733315, 37.644588);
                        var mapOptions = {
                            zoom: 15,
                            center: venuePosition,
                            mapTypeId: google.maps.MapTypeId.ROADMAP
                        };

                        locationMap = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);
                        var venueMarker = new google.maps.Marker({
                            title:'Swissotel Krasnye Holmy',
                            position: venuePosition,
                            icon:   new google.maps.MarkerImage('http://russianpaymentsforum.ticketforevent.com/img/marker_venue.png', new google.maps.Size(36, 50), new google.maps.Point(0,0), new google.maps.Point(18, 50)),
                            shadow: new google.maps.MarkerImage('http://russianpaymentsforum.ticketforevent.com/img/shadow_venue.png', new google.maps.Size(59, 50), new google.maps.Point(0,0), new google.maps.Point(18, 50))
                        });
                        venueMarker.setMap(locationMap);

                        var infowindow = new google.maps.InfoWindow({content: '<br/><b>Swissotel Krasnye Holmy</b><br/>naberezhnaya Kosmodamianskaya 52 строение 6'});
                        google.maps.event.addListener(venueMarker, 'click', function() {
                            if (locationInfo) locationInfo.close();
                            locationInfo = infowindow;
                            infowindow.open(locationMap, venueMarker);
                        });

                    }
                function loadScript() {
                    var script = document.createElement('script');
                    script.type = 'text/javascript';
                    script.src = 'https://maps.googleapis.com/maps/api/js?v=3.10&sensor=false&' +
                            'callback=locationMapInitialize';
                    document.body.appendChild(script);
                }

                $(function(){
                    $('.location-map-static').click(function(){
                        openMapDialog();
                        if(!locationMap) loadScript();
                        else google.maps.event.trigger(locationMap, 'resize');
                    });
                })
            </script>

            <div id="dialog-content" style="display: none">
                <div class="location-map-dialog" id="map-canvas"></div>
            </div>

            <div class="location-map-static" style="background-image: url('https://maps.google.com/maps/api/staticmap?language=en&key=AIzaSyB0ywAMo-aWPm4k_K4JkFnCKetpQzUJWN8&zoom=15&size=260x260&markers=icon:http://russianpaymentsforum.ticketforevent.com%2Fimg%2Fmarker_venue.png|shadow:http://russianpaymentsforum.ticketforevent.com%2Fimg%2Fshadow_venue.png|55.733315,37.644588&sensor=false');">
                <div class="location-zoom-button ic tooltip" title="Zoom in"></div>
