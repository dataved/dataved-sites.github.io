		<script type="text/javascript">
		function RedrawChart() {
			var url = "";
			var frm = document.forms['nvgraph'];
			if (frm) {
				url = "http://gr04.finmarket.ru/charts/ExchangeRates.aspx?op=26940";
				url += "&val=" + frm.val[frm.val.selectedIndex].value;
				url += "&vol=" + frm.opt[frm.opt.selectedIndex].value;
				for (var i = 0; i < frm.aver.length; i++) {
					if (frm.aver[i].checked) url += "&comp=" + frm.aver[i].value;
				}
				document.getElementById("NVChart").src = url;
			}
			return false;
		}
		</script>
	
