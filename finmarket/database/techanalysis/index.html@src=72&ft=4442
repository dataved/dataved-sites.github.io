function ClearForm() {
	var frm = document.frmChart2;
	for (i = 0; i < 3; i++) {
		frm.avgsl[i].selectedIndex = 0;
		frm.slper[i].value = "";
		frm.devtype[i].selectedIndex = 0;
		frm.devdist[i].value = "";
