<script language="javascript">
function SelAll() {
	var frm = document.forms("ftids");
	if (frm) {
		var coll = frm.ftid;
		if (coll) {
			for (var i = 0; i < coll.length; i++) {
				coll[i].checked = true;
			}
		}
	}
}
function ClearAll() {
	var frm = document.forms("ftids");
	if (frm) {
		var coll = frm.ftid;
		if (coll) {
			for (var i = 0; i < coll.length; i++) {
				coll[i].checked = false;
			}
		}
	}
}
</script>

